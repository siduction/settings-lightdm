#!/bin/sh
# prerm script for #PACKAGE#

set -e

case "$1" in
    remove|upgrade|deconfigure)
        [ -f /etc/lightdm/lightdm-kde-greeter.conf.orig ] && mv /etc/lightdm/lightdm-kde-greeter.conf.orig /etc/lightdm/lightdm-kde-greeter.conf;
        ;;
    failed-upgrade)
       ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0
